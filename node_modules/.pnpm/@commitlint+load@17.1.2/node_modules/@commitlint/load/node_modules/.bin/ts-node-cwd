#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../ts-node@10.9.1_44umifdr5jye7imcdire4yqpai/node_modules/ts-node/dist/bin-cwd.js" "$@"
else
  exec node  "$basedir/../../../../../../ts-node@10.9.1_44umifdr5jye7imcdire4yqpai/node_modules/ts-node/dist/bin-cwd.js" "$@"
fi
